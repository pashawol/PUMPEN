mixin s-order-history()
	// start s-order-history
	+b.SECTION.s-order-history.section#s-order-history&attributes(attributes)
		.container
			+b.section-title
				h2 Meine Bestellhistorie
			mixin order-date()
				+e.main-head
					+e.date.acc-head
