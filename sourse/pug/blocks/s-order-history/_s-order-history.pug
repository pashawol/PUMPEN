							tr
								td Bestelldatum <span>TT.MM.JJJJ</span>
								td Bestellnummer: <span>XXXXXXXX</span>
								td Lieferung an: <span>Name Nachname</span>
								td
					.acc-body
						+e.order-item
							table
								tr
									td 
										.img-wrap
											img(src="img/@2x/basket-i.jpg" alt="")
