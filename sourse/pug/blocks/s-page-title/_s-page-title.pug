mixin s-page-title(t2="Registrierung", t3="Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore")
	// start s-page-title
	+b.s-page-title.section#s-page-title&attributes(attributes)
		.container
			block
			+e.title.h1= t2
			+e.subtitle= t3
			
	// end s-page-title

