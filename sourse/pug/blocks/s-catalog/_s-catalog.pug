mixin s-catalog-builder
	// start s-catalog
	+b.s-catalog.section&attributes(attributes) 
		block
		.row
			- for (var x = 0; x < 8; x++)
				+prod()(class="col-lg-3 col-6")
		+e.btn--more(href="#") Mehr 
	// end s-catalog

