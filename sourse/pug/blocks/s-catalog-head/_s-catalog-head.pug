mixin s-catalog-head()
	// start s-catalog-head
	+b.s-catalog-head.section#s-catalog-head&attributes(attributes)
		.container
			+e.btn-wrap.tabs__caption
				+e.btn.tabs__btn.active(type="button") Ersatzteile
				+e.btn.tabs__btn(type="button") Pumpen
				+e.btn.tabs__btn(type="button") zubehör
			.d-md-none
				+e.input-wrap
					+e.SELECT.input.form-control

							option()  Ersatzteile
							option() Pumpen
							option() zubehör
						 
	// end s-catalog-head


mixin s-catalog-head-builder()
	// start s-catalog-head
	+b.s-catalog-head--builder.section#s-catalog-head&attributes(attributes)
		.container
 
			+e.btn-wrap.tabs__caption
				+e.btn.tabs__btn.active(type="button") Informationen
				+e.btn.tabs__btn(type="button") Ersatzteile
				+e.btn.tabs__btn(type="button") NEUKAUF
			.d-md-none
				+e.input-wrap
					+e.SELECT.input.form-control

							option()  Informationen
							option() Ersatzteile
							option()  NEUKAUF
						 
	// end s-catalog-head

